{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "outDir": "../canvas_ux/lib",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "types": [
      "svelte",
      "node"
    ],
    "importsNotUsedAsValues": "remove",
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["../canvas_ux/node_modules/@types"]
  },
  "include": [
    "../canvas_ux/src/**/*.ts",
    "../canvas_ux/src/**/*.tsx",
    "../canvas_ux/src/**/*.svelte"
  ],
  "exclude": [
    "../canvas_ux/node_modules"
  ]
}
